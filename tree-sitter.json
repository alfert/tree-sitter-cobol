{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "cobol",
      "camelcase": "COBOL",
      "title": "COBOL",
      "scope": "source.cobol",
      "file-types": [
        "CPY",
        "COB",
        "CBL"
      ],
      "injection-regex": "^cobol$",
      "class-name": "TreeSitterCobol"
    }
  ],
  "metadata": {
    "version": "0.2.0",
    "license": "MIT",
    "description": "COBOL grammar for the tree-sitter parsing library",
    "authors": [
      {
        "name": "Yutaro Sakamoto",
        "url": "https://github.com/yutaro-sakamoto"
      }
    ],
    "links": {
      "repository": "https://github.com/yutaro-sakamoto/tree-sitter-cobol"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}